
<div class="visits-dash col-md-6">
	<div class="lists">Last Visits</div>
	<% @visit.last_created_visits(5).each do |v| %>
		<li id="<%= v.id %>" class="lists">
			<%= v.title %> -
			<%= v.description %>
			<%= link_to 'Edit', edit_admin_dashboard_path(v.id) %>
		</li>
	<% end %>	
</div>

<div class="reservation-dash col-md-6">
	<div class="lists">Last reservation</div>
	<% @reservation.last_created_reservation(5).each do |r| %>
		<li id="<%= r.id %>" class="lists">
			<%= r.title %> -
			<%= r.email %>
			<%= link_to 'Edit', edit_admin_reservation_path(r.id) %>
		</li>
	<% end %>
</div>

<div class="paginate apple_pagination col-md-12"><%= will_paginate @visit %></div>

<div id="calendar" class="col-md-12">
	<%= javascript_tag do -%>
	var visits = <%= @calendar_items.html_safe %>;
	<% end -%>
</div>


	
</div>